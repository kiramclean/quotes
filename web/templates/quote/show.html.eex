<h2>Show quote</h2>

<ul>

  <li>
    <strong>Quote:</strong>
    <%= @quote.quote %>
  </li>

  <li>
    <strong>Author:</strong>
    <%= @quote.author %>
  </li>

  <li>
    <strong>Source:</strong>
    <%= @quote.source %>
  </li>

</ul>

<%= link "Edit", to: quote_path(@conn, :edit, @quote) %>
<%= link "Back", to: quote_path(@conn, :index) %>
